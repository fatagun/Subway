{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "InMemoryCache": {
    "EnableLogging": true,
    "SlidingExpirationInSeconds": 600,
    "CacheSize": 20000,
    "AbsoluteExpirationInSeconds": 1800,
    "ExpirationScanFrequency" :  60
  }
}
